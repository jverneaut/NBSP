{
  "name": "NBSP",
  "version": "1.0",
  "description": "Find all the nasty &nbsp; elements on your pages.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png",
    "default_title": "NBSP"
  },
  "manifest_version": 2
}